<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="report_estate_property_offers" model="ir.actions.report">
        <field name="name">Estate Property Offers</field>
        <field name="model">real.estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">real_estate.property_template</field>
        <field name="report_file">real_estate.property_template</field>
        <field name="print_report_name">'Property - %s' % (object.name).replace('/','')</field>
        <field name="paperformat_id" ref="base.paperformat_us"/>
        <field name="binding_model_id" ref="model_real_estate_property"/>
        <field name="binding_type">report</field>
    </record>
    <record id="report_estate_property_salesman" model="ir.actions.report">
        <field name="name">Property Report</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">real_estate.user_property_template</field>
        <field name="report_file">user_property_template</field>
        <field name="print_report_name">'User - %s' % (object.name).replace('/','')</field>
        <field name="paperformat_id" ref="base.paperformat_us"/>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
