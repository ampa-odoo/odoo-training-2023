<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="offer_azure_villa_1" model="real.estate.property.offer">
            <field name="price">10000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12"/>
            <field name="property_id" ref="villa"/>
            <field name="date_deadline" eval="str(datetime.now().year)+'-01-31'"/>
        </record>
        <record id="offer_azure_villa_2" model="real.estate.property.offer">
            <field name="price">1500000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12"/>
            <field name="property_id" ref="villa"/>
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-11'"/>
        </record>
        <record id="offer_deco_villa" model="real.estate.property.offer">
            <field name="price">1500001</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_2"/>
            <field name="property_id" ref="villa"/>
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-12'"/>
        </record>
        <function model="real.estate.property.offer" name="action_accept">
            <value eval="[ref('offer_deco_villa')]"/>
        </function>
        <function model="real.estate.property.offer" name="action_refuse">
            <value eval="[ref('offer_azure_villa_1'), ref('offer_azure_villa_2')]"/>
        </function>
        <function model="estate.property" name="action_sold">
            <value eval="[ref('villa')]"/>
        </function>
        <record id="offer_cabin_1" model="real.estate.property.offer">
            <field name="price">60000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12"/>
            <field name="property_id" ref="cabin"/>
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-12'"/>
        </record>
        <record id="offer_cabin_2" model="real.estate.property.offer">
            <field name="price">75000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12"/>
            <field name="property_id" ref="cabin"/>
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-12'"/>
        </record>
    </data>
</odoo>