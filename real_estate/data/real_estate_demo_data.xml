<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <data>

        <!-- Property Types -->
        <record id="property_residential" model="estate.property.type">
            <field name="name">Residential</field>
        </record>

        <record id="property_commercial" model="estate.property.type">
            <field name="name">Commercial</field>
        </record>

        <record id="property_industrial" model="estate.property.type">
            <field name="name">Industrial</field>
        </record>

        <record id="property_land" model="estate.property.type">
            <field name="name">Land</field>
        </record>
        <record model="estate.property.type" id="estate_type_demo_data1">
            <field name="name">House</field>
        </record>

        <record model="estate.property.type" id="estate_type_demo_data2">
            <field name="name">Appartment</field>
        </record>

        <record model="estate.property.type" id="estate_type_demo_data3">
            <field name="name">Bungalows</field>
        </record>

        <record model="estate.property.tag" id="estate_tag_demo_data1">
            <field name="name">Cozy</field>
        </record>

        <record model="estate.property.tag" id="estate_tag_demo_data2">
            <field name="name">Renovated</field>
        </record>


        <!-- Properties -->
        <record id="villa" model="estate.property">
            <field name="name">Big Villa</field>
            <field name="state">new</field>
            <field name="description">A nice and big villa</field>
            <field name="postcode">12345</field>
            <field name="date_availability">2020-02-02</field>
            <field name="expected_price">1600000</field>
            <field name="bedrooms">6</field>
            <field name="living_area">100</field>
            <field name="facades">4</field>
            <field name="garage">True</field>
            <field name="garden">True</field>
            <field name="garden_area">100000</field>
            <field name="garden_orientation">south</field>
            <field name="property_type_id" ref="property_residential" />
            <field name="salesman_id" ref="base.user_admin" />
            <field name="image" type="base64" file="real_estate/static/src/img/villa.jpg" />
        </record>

        <record id="trailer" model="estate.property">
            <field name="name">Trailer Home</field>
            <field name="state">cancel</field>
            <field name="description">Home in a trailer park</field>
            <field name="postcode">54321</field>
            <field name="date_availability">1970-01-01</field>
            <field name="expected_price">100000</field>
            <field name="selling_price">120000</field>
            <field name="bedrooms">1</field>
            <field name="living_area">10</field>
            <field name="facades">4</field>
            <field name="garage">False</field>
            <field name="garden">True</field>
            <field name="property_type_id" ref="property_residential" />
            <field name="salesman_id" ref="base.user_admin" />
            <field name="image" type="base64" file="real_estate/static/src/img/house.jpg" />

        </record>

        <record id="space_ship" model="estate.property">
            <field name="name">International Space Station</field>
            <field name="state">new</field>
            <field name="description">Aliens sometimes come visit</field>
            <field name="postcode">23456</field>
            <field name="date_availability">2030-12-31</field>
            <field name="expected_price">4589000</field>
            <field name="property_type_id" ref="property_industrial" />
            <field name="salesman_id" ref="base.user_admin" />
            <field name="image" type="base64" file="real_estate/static/src/img/space.jpg" />

        </record>

        <record id="cabin" model="estate.property">
            <field name="name">Cozy Cabin</field>
            <field name="state">sold</field>
            <field name="description">Small cabin by lake</field>
            <field name="postcode">10000</field>
            <field name="date_availability">2020-01-01</field>
            <field name="expected_price">80000</field>
            <field name="bedrooms">1</field>
            <field name="living_area">10</field>
            <field name="facades">4</field>
            <field name="garage">False</field>
            <field name="garden">True</field>
            <field name="property_type_id" ref="property_residential" />
            <field name="salesman_id" ref="base.user_admin" />
            <field name="image" type="base64" file="real_estate/static/src/img/house.jpg" />
        </record>

        <record model="estate.property" id="estate_demo_data">
            <field name="name">Harsh</field>
            <field name="description"> 11 Floor Tower</field>
            <field name="postcode">123456</field>
            <field name="bedrooms"> 3</field>
            <field name="living_area"> 6</field>
            <field name="expected_price">23999</field>
            <field name="selling_price">458888</field>
            <!-- <field name="date_availability">2002/2/23</field> -->
            <field name="garden_area">123</field>
            <field name="tag_ids"
                eval="[(6,0, [ref('real_estate.estate_tag_demo_data1'), ref('real_estate.estate_tag_demo_data2')])]"></field>
            <field name="image" type="base64" file="real_estate/static/src/img/flat.jpg" />
        </record>
        <record model="estate.property" id="estate_demo_data3">
            <field name="name">Arena</field>
            <field name="description"> 7 tower campus</field>
            <field name="postcode">654321</field>
            <field name="bedrooms">8</field>
            <field name="living_area"> 16</field>
            <field name="expected_price">76999</field>
            <field name="selling_price">258888</field>
            <!-- <field name="date_availability">2002/2/23</field> -->
            <field name="garden_area">123</field>
            <field name="tag_ids" eval="[(6,0, [ref('real_estate.estate_tag_demo_data1')])]"></field>
            <field name="image" type="base64" file="real_estate/static/src/img/flat.jpg" />

        </record>
        <record model="estate.property" id="estate_demo_data1">
            <field name="name">PentHouse</field>
            <field name="postcode">121456</field>
            <field name="description"> 5 BHK House</field>
            <field name="bedrooms"> 7</field>
            <field name="living_area"> 16</field>
            <field name="expected_price">63999</field>
            <field name="selling_price">458688</field>
            <!-- <field name="date_availability">2002/2/23</field> -->
            <field name="garden_area">123</field>
            <field name="tag_ids" eval="[(6,0, [ref('real_estate.estate_tag_demo_data2')])]"></field>
            <field name="image" type="base64" file="real_estate/static/src/img/penthouse.jpg" />

        </record>
        <record model="estate.property" id="estate_demo_data2">
            <field name="name">Small Bungalows</field>
            <field name="postcode">125456</field>
            <field name="description"> Raw Houses</field>
            <field name="bedrooms">65</field>
            <field name="living_area"> 3</field>
            <field name="expected_price">28799</field>
            <field name="selling_price">758888</field>
            <!-- <field name="date_availability">2002/2/23</field> -->
            <field name="garden_area">123</field>
            <field name="tag_ids"
                eval="[(6,0, [ref('real_estate.estate_tag_demo_data1'), ref('real_estate.estate_tag_demo_data2')])]"></field>
                <field name="image" type="base64" file="real_estate/static/src/img/bunglow.jpg" />
        </record>


        <!-- Offers -->
        <!-- Villa Offers and Offer Status -->
        <record id="offer_azure_villa_1" model="estate.property.offer">
            <field name="price">10000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12" />
            <field name="property_id" ref="villa" />
            <field name="date_deadline" eval="str(datetime.now().year)+'-01-31'" />
        </record>

        <record id="offer_azure_villa_2" model="estate.property.offer">
            <field name="price">1500000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12" />
            <field name="property_id" ref="villa" />
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-11'" />
        </record>

        <record id="offer_deco_villa" model="estate.property.offer">
            <field name="price">1500001</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_2" />
            <field name="property_id" ref="villa" />
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-12'" />
        </record>


        <!-- Cabin Offers and Offer Status -->
        <record id="offer_cabin_1" model="estate.property.offer">
            <field name="price">60000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12" />
            <field name="property_id" ref="cabin" />
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-12'" />
        </record>

        <record id="offer_cabin_2" model="estate.property.offer">
            <field name="price">75000</field>
            <field name="validity">14</field>
            <field name="partner_id" ref="base.res_partner_12" />
            <field name="property_id" ref="cabin" />
            <field name="date_deadline" eval="str(datetime.now().year)+'-02-12'" />
        </record>

    </data>
</odoo>