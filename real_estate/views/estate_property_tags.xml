<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estate_property_tags_action" model="ir.actions.act_window">
        <field name="name">Real Estate Tags</field>
        <field name="res_model">estate.property.tag</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                There is no record of any Property 
            </p>
        </field>
    </record>

    <!-- Tree view -->
    <record id="Estate_property_tags_tree_view" model="ir.ui.view">
        <field name="name">Estate property tags</field>
        <field name="model">estate.property.tag</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name" string="tags"/>
            </tree>
        </field>
    </record>
</odoo>