<?xml version="1.0" encoding="UTF-8"?>
<odoo>

<!--tree view_mode -->

    <record id="user_data_view_tree" model="ir.ui.view">
        <field name="name">user.data.view.tree</field>
        <field name="model">users.users</field>
        <field name="arch" type="xml">
            <tree string="Estate_Property">
               <field name="name"/>
            </tree>
        </field>
    </record>


<!-- form view -->

<record id="user_data_view_form" model="ir.ui.view">
        <field name="name">user.data.view.form</field>
        <field name="model">users.users</field>
        <field name="arch" type="xml">
            <form string="Users"> 
                <sheet>
                            <h1>
                                <field name="name"/>
                            </h1>                 
                        <notebook>
                            <page string="Properties">

                                <field name="property_ids">
                                    <tree>
                                        <field name="name"/>
                                        <field name="expected_price"/>
                                        <field name="status"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                </sheet>
            </form>
        </field>
    </record>
</odoo>