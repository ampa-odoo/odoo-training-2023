<?xml version="1.0"?>
    <odoo>
    <record id="estate_sales" model="ir.ui.view">
            <field name="name">estate.sale.form.view</field>
            <field name="model">estate.property.sales</field>
           
            <field name="arch" type="xml">
                <form string="type">
                        <sheet>
                            <h1>
                                <label for="name" string="sale type Name" />
                                <field name="name" nolable="1"/>
                            </h1>
                        </sheet>
                </form>
            </field>
             
    </record>
    <record id="estate_sale_tree_view" model="ir.ui.view">
        <field name="name">real.estate.view.tree</field>
        <field name="model">estate.property.sales</field>
        <field name="arch" type="xml">
            <tree string="Estate_Property_sales">
                <field name="name"/>
                </tree>
                </field>
                </record>

    </odoo>