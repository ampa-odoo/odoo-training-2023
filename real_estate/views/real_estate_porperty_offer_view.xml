<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- FOR TREE VIEW -->
    <record id="real_estate_property_tags_view_tree" model="ir.ui.view">
        <field name="name">real_estate_property_Offer_tree_view</field>
        <field name="model">real.estate.property.offer</field>
        <field name="arch" type="xml">
            <tree>
                <field name="price"/>
                <field name="status"/>
                <field name="partner_id"/>
                <field name="property_id"/>
            </tree>
        </field>
    </record>
    <record id="real_estate_property_tags_view_form" model="ir.ui.view">
        <field name="name">real_estate_property_Offer_form_view</field>
        <field name="model">real.estate.property.offer</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="price" />
                        <field name="partner_id" />
                        <field name="status" />
                        <!-- <field name="property_id" /> -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>