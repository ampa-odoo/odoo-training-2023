<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="real_estate_type_list_view" model="ir.ui.view">
        <field name="name">Real Estate Type List View</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">
            <tree string="Property Type">
                <field name="name" />
                <field name="sequence" widget="handle" />
            </tree>
        </field>
    </record>

    <record id="real_estate_type_form_view" model="ir.ui.view">
        <field name="name">Real Estate Type Form View</field>
        <field name="model">estate.property.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <h1><field name="name"/></h1>
                    <notebook>
                        <page string="Properties">
                            <field name="property_type_name_list"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
