<?xml version="1.0"?>
  <odoo>
    <record id="estate_property_action" model="ir.actions.act_window">
      <field name="name">real estate action</field>
      <field name="res_model">estate.property</field>
      <field name="view_mode">tree,form</field>
      <field name="help" type="html">
        <p class="o_view_nocontent_smiling_face">
                  Create a new quotation, the first step of a new sale!
        </p>
      </field>
    </record>
   
    <record id="real_estate_search" model="ir.ui.view">
              <field name="name">estate.property.search</field>
              <field name="model">estate.property</field>
              <field name="arch" type="xml">
                    <search string="real estate">
                    <field name="name"/>
                    <field name="postcode"/>
                    <field name="expeccted_price"/>
                    <field name="date_availability"/>
                    <field name="selling_price"/>
                    <field name="description"/>
                    <field name="type_id"/> 
                   <!--<field name="tag_id" widget="many2many_tags"/> -->

                    <field name="bedroom"/>
                    <field name="living_area"/>
                    <field name="facades"/>
                    <field name="garage"/>
                    <field name="garden"/>
                    <field name="garden_area"/>
                    <field name="garden_orientation"/>
                    <field name="state"/>
                      <separator/>
                      <filter string="price filter" name="expeccted_price" domain="[('expeccted_price', '=', 120000)]"/>
                      <filter string="pin" name="postcode" domain="[('postcode', '!=', 656565)]"/>
                      <filter name="filter_date_availability" date="date_availability" string="available Date"/>
                      <filter name="filter_create_date" date="create_date" string=" Date"/>
                      <!--<group expand="2" string="Group By">
                          <filter string="expected price" name="expeccted_price" context="{'group_by':'expeccted_price', 'residual_visible':True}"/>
                      </group>-->
                  </search>
              </field>
          </record>
  </odoo>