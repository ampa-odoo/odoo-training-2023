<?xml version="1.0"?>
    <odoo>
    <record id="estate_offer" model="ir.ui.view">
            <field name="name">estate.offer.form.view</field>
            <field name="model">estate.property.offer</field>
           
            <field name="arch" type="xml">
                <form string="type">
                        <sheet>
                            <group>
                                
                                <field name="price"/>
                                <field name="partner_id"/>
                               <field name="status"/>
                            </group>
                        </sheet>
                </form>
            </field>
             
    </record>
    <record id="estate_offer_tree_view" model="ir.ui.view">
        <field name="name">estate.offer.view.tree</field>
        <field name="model">estate.property.offer</field>
        <field name="arch" type="xml">
            <tree string="Estate_Property_offer">
                <field name="price"/>
                 <field name="partner_id"/>
                <field name="status"/>
                </tree>
                </field>
                </record>

    </odoo>