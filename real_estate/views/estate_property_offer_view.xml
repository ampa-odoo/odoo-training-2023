<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="model_estate_property_offer_view_form" model="ir.ui.view">
            <field name="name">Real Estate Form</field>
            <field name="model">estate.property.offer</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="price"/>
                    <field name="partner_id"/>
                    <button name="action_accept" title="accept" type="object" icon="fa-check"/>
                    <button name="action_refuse" title="refuse" type="object" icon="fa-times"/>
                    <field name="status" optional='hide'/>
                    <field name="validity"/>
                    <field name="date_deadline"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
