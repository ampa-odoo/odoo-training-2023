<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <!-- treee view -->
         <record id="estate_property_type_view" model="ir.ui.view">
         <field name="name">Properties</field>
         <field name="model">estate.property</field>
             <field name="arch" type="xml">
                 <tree string="...">    
                    <field name="name" />
                    <field name="description" />
                    <field name="bedrooms" />
                    <field name="expected_price"/>
                    <field name="garden" />
                    <field name="living_area"/>
                    <field name="tag_ids" widget="many2many_tags"/>
                 </tree>
             </field>
        </record>


        <record id="property_type_action" model="ir.actions.act_window">
            <field name="name">Property types</field>
            <field name="res_model">estate.property.type</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Add a new property type
                </p>
            </field>
        </record>

        <record id="property_tags_action" model="ir.actions.act_window">
            <field name="name">Property tags</field>
            <field name="res_model">estate.property.tags</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Add a new property tag
                </p>
            </field>
        </record>

    </data>
</odoo>