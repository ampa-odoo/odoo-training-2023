<?xml version="1.0" encoding="utf-8"?>
<odoo>   
    <record id="estate_property_wizard_view_form" model="ir.ui.view">  
        <field name="name">estate.property.wizard.form</field>
        <field name="model">estate.property</field>
        <field name="arch" type="xml">
            <form string="Test">
                <group class="oe_title">
                    <field name="name"/>
                </group>
            </form>
        </field>
    </record>

    <record id="estate_property_wizard_action" model="ir.actions.act_window">
        <field name="name">Estate Property Wizard</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">estate.property</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="estate_property_wizard_view_form"/>
        <field name="target">new</field>
    </record>
</odoo>
                